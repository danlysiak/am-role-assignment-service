{
  "title": "must successfully create single Role Assignment with RoleTypeId as case",
  "_guid_": "S-121",
  "_extends_": "F-009_Test_Data_Base",
  "specs": [
    "contains a single salaried judge Role Assignment",
    "contains RoleTypeId as 'CASE'"
  ],
  "request": {
    "body": {
      "roleRequest": {
        "assignerId": "${[scenarioContext][testData][users][invokingUser][id]}",
        "reference": "${[scenarioContext][customValues][generateUUID]}"
      },
      "requestedRoles": [
        {
          "_extends_": "RequestedRoleSalariedJudge",
          "actorId": "${[scenarioContext][testData][request][body][roleRequest][reference]}",
          "roleType": "CASE",
          "roleName": "salaried-judge",
          "grantType": "SPECIFIC",
          "attributes":
          {
            "caseType": "IDAM",
            "caseId": "1234567890123456"
          }
        }
      ]
    }
  },
  "expectedResponse": {
    "_extends_": "Common_201_Response",
    "headers": {
      "Content-Length": "[[ANYTHING_PRESENT]]",
      "Content-Type": "application/json"
    },
    "body": {
      "roleAssignmentResponse": {
        "roleRequest": {
          "_extends_": "ResponseRoleRequest"
        },
        "requestedRoles": [
          {
            "_extends_": "ResponseRequestedRoleSalariedJudge",
            "roleType": "CASE",
            "roleName": "salaried-judge",
            "grantType": "SPECIFIC",
            "attributes":
            {
              "caseType": "IDAM",
              "caseId": "1234567890123456"
            }
          }
        ]
      }
    }
  }
}
