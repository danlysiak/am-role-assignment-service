package validationrules.test

import  uk.gov.hmcts.reform.roleassignment.launchdarkly.FeatureFlagEnum;
import  uk.gov.hmcts.reform.roleassignment.launchdarkly.LDFeatureFlag;



rule "test_feature"
when

     $f : LDFeatureFlag(status && flagName == FeatureFlagEnum.getLDFlag.getValue())

then
     System.out.println("Feature Toggle enabled :: " + $f.getFlagName());

end;
